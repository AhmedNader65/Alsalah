<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.crazyidea.alsalah.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_navigation_home_to_navigation_azkar_details"
            app:destination="@id/navigation_azkar_details" />
        <action
            android:id="@+id/action_navigation_home_to_compassFragment2"
            app:destination="@id/compassFragment2" />
        <action
            android:id="@+id/action_navigation_home_to_calendarFragment"
            app:destination="@id/calendarFragment" />
    </fragment>

    <fragment
        android:id="@+id/navigation_azkar"
        android:name="com.crazyidea.alsalah.ui.azkar.main.AzkarFragment"
        android:label="@string/title_azkar"
        tools:layout="@layout/fragment_azkar">
        <action
            android:id="@+id/action_navigation_azkar_to_navigation_azkar_details"
            app:destination="@id/navigation_azkar_details" />
        <action
            android:id="@+id/action_navigation_azkar_to_sebhaFragment"
            app:destination="@id/sebhaFragment" />
    </fragment>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.crazyidea.alsalah.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/navigation_menu"
        android:name="com.crazyidea.alsalah.ui.menu.MenuFragment"
        android:label="MenuFragment"
        tools:layout="@layout/fragment_menu">
        <action
            android:id="@+id/action_navigation_menu_to_fajrListFragment"
            app:destination="@id/fajrListFragment" />
        <action
            android:id="@+id/action_navigation_menu_to_settingFragment"
            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/action_navigation_menu_to_followUsFragment2"
            app:destination="@id/followUsFragment" />
        <action
            android:id="@+id/action_navigation_menu_to_technicalSupportFragment"
            app:destination="@id/technicalSupportFragment" />
        <action
            android:id="@+id/action_navigation_menu_to_whoAreWeFragment"
            app:destination="@id/whoAreWeFragment" />
        <action
            android:id="@+id/action_navigation_menu_to_compassFragment2"
            app:destination="@id/compassFragment2" />
        <action
            android:id="@+id/action_navigation_menu_to_fawaedFragment"
            app:destination="@id/fawaedFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.crazyidea.alsalah.ui.setting.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting">

        <action
            android:id="@+id/action_settingFragment_to_prayerTimingFragment"
            app:destination="@id/prayerTimingFragment" />
        <action
            android:id="@+id/action_settingFragment_to_mazhabFragment"
            app:destination="@id/mazhabFragment" />
        <action
            android:id="@+id/action_settingFragment_to_refactorPrayerTimingFragment"
            app:destination="@id/refactorPrayerTimingFragment" />
        <action
            android:id="@+id/action_settingFragment_to_poleCalculateWayFragment"
            app:destination="@id/poleCalculateWayFragment" />
        <action
            android:id="@+id/action_settingFragment_to_resetFragment"
            app:destination="@id/resetFragment" />
        <action
            android:id="@+id/action_settingFragment_to_locationFragment"
            app:destination="@id/locationFragment" />
        <action
            android:id="@+id/action_settingFragment_to_azkarSettingFragment"
            app:destination="@id/azkarSettingFragment" />
        <action
            android:id="@+id/action_settingFragment_to_azanSettingFragment"
            app:destination="@id/azanSettingFragment" />
        <action
            android:id="@+id/action_settingFragment_to_languageFragment"
            app:destination="@id/languageFragment" />
    </fragment>

    <fragment
        android:id="@+id/compassFragment2"
        android:name="com.crazyidea.alsalah.ui.compass.CompassScreen"
        android:label="CompassScreen"
        tools:layout="@layout/fragment_compass" />
    <fragment
        android:id="@+id/fajrListFragment"
        android:name="com.crazyidea.alsalah.ui.fajrlist.FajrListFragment"
        android:label="FajrListFragment"
        tools:layout="@layout/fragment_fajr_list" />
    <fragment
        android:id="@+id/navigation_azkar_details"
        android:name="com.crazyidea.alsalah.ui.azkar.azkar_details.AzkarDetailsFragment"
        tools:layout="@layout/fragment_azkar_details">
        <action
            android:id="@+id/action_navigation_azkar_details_to_azkarMenuFragment"
            app:destination="@id/azkar_list" />
        <argument
            android:name="category"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/azkar_list"
        android:name="com.crazyidea.alsalah.ui.azkar.azkar_menu.AzkarMenuFragment"
        android:label="AzkarListFragment"
        tools:layout="@layout/fragment_azkar_list" >
        <argument
            android:name="category"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/sebhaFragment"
        android:name="com.crazyidea.alsalah.ui.azkar.sebha.SebhaFragment"
        android:label="SebhaFragment"
        tools:layout="@layout/fragment_sebha" >
        <action
            android:id="@+id/action_sebhaFragment_to_azkar_list"
            app:destination="@id/azkar_list" />
    </fragment>
    <fragment
        android:id="@+id/followUsFragment"
        android:name="com.crazyidea.alsalah.ui.followUs.FollowUsFragment"
        android:label="FollowUsFragment"
        tools:layout="@layout/fragment_follow_us" />
    <fragment
        android:id="@+id/technicalSupportFragment"
        android:name="com.crazyidea.alsalah.ui.technicalSupport.TechnicalSupportFragment"
        android:label="TechnicalSupportFragment"
        tools:layout="@layout/fragment_technical_support" />
    <fragment
        android:id="@+id/whoAreWeFragment"
        android:name="com.crazyidea.alsalah.ui.whoAreWe.WhoAreWeFragment"
        android:label="WhoAreWeFragment"
        tools:layout="@layout/fragment_who_are_we" />
    <fragment
        android:id="@+id/calendarFragment"
        android:name="com.crazyidea.alsalah.ui.calendar.CalendarFragment"
        android:label="CalendarFragment"
        tools:layout="@layout/fragment_calendar" />
    <fragment
        android:id="@+id/fawaedFragment"
        android:name="com.crazyidea.alsalah.ui.fawaed.FawaedFragment"
        android:label="FawaedFragment"
        tools:layout="@layout/fragment_fawaed" />
    <fragment
        android:id="@+id/prayerTimingFragment"
        android:name="com.crazyidea.alsalah.ui.prayerTiming.PrayerTimingFragment"
        android:label="PrayerTimingFragment"
        tools:layout="@layout/fragment_choose_language" />
    <fragment
        android:id="@+id/mazhabFragment"
        android:name="com.crazyidea.alsalah.ui.mazhab.MazhabFragment"
        android:label="MazhabFragment"
        tools:layout="@layout/fragment_choose_mazhab" />
    <fragment
        android:id="@+id/refactorPrayerTimingFragment"
        android:name="com.crazyidea.alsalah.ui.refactorPrayerNotification.RefactorPrayerTimingFragment"
        android:label="RefactorPrayerTimingFragment"
        tools:layout="@layout/fragment_prayer_time_notification" />
    <fragment
        android:id="@+id/poleCalculateWayFragment"
        android:name="com.crazyidea.alsalah.ui.poleCalculateWay.PoleCalculateWayFragment"
        android:label="PoleCalculateWayFragment"
        tools:layout="@layout/fragment_choose_pole_calculate_way" />
    <fragment
        android:id="@+id/resetFragment"
        android:name="com.crazyidea.alsalah.ui.reset.ResetFragment"
        android:label="ResetFragment"
        tools:layout="@layout/fragment_reset" />
    <fragment
        android:id="@+id/locationFragment"
        android:name="com.crazyidea.alsalah.ui.location.LocationFragment"
        android:label="LocationFragment"
        tools:layout="@layout/fragment_location" />
    <fragment
        android:id="@+id/azkarSettingFragment"
        android:name="com.crazyidea.alsalah.ui.azkarSetting.AzkarSettingFragment"
        android:label="AzkarSettingFragment"
        tools:layout="@layout/fragment_azkar_setting" />
    <fragment
        android:id="@+id/languageFragment"
        android:name="com.crazyidea.alsalah.ui.language.LanguageFragment"
        android:label="LanguageFragment"
        tools:layout="@layout/fragment_choose_language" />
    <fragment
        android:id="@+id/azanSettingFragment"
        android:name="com.crazyidea.alsalah.ui.azanSetting.AzanSettingFragment"
        android:label="AzanSettingFragment"
        tools:layout="@layout/fragment_azan_setting" />

</navigation>